import ../core

fn TestLen() {
        list = ()

        length <= len($list)

        if $length != "0" {
                echo "Invalid length \"" + $length + "\". Expect 0"
                exit
        } else {
                echo "SUCCESS"
        }

        list = ("0")

        length <= len($list)

        if $length != "1" {
                echo "Invalid length \"" + $length + "\". Expect 1"
                exit
        } else {
                echo "SUCCESS"
        }
}

TestLen()
